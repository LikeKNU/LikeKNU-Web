{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://server.like-knu.com/api/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "https://server.like-knu.com/api/:path*/"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
